	<queryString>
		<![CDATA[select * from prod_compra
inner join produto on prod_Compra.Cod_Prod = produto.Cod_Prod
inner join compra on prod_compra.Cod_Compra = compra.Cod_Compra
inner join forma_pag on compra.Cod_Forma_Pag = forma_pag.Cod_Forma_Pag
inner join funcionario on funcionario.Cod_Func = compra.Cod_Func
inner join cliente on cliente.Cod_Cli = compra.Cod_Cli
left join clube_fidelidade on cliente.Cod_Cli = clube_fidelidade.Cod_Cli
where prod_compra.Cod_Compra = $P{id_compra}]]>
	</queryString>
